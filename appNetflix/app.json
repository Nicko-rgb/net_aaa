{
  "expo": {
    "name": "Netflix",
    "slug": "Netflix",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/img/Logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/Logo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nixonml.Netflix"
    },
    "web": {
      "favicon": "./assets/img/Logo.png"
    },
    "plugins": [
      "expo-video"
    ],
    "owner": "nixonml",
    "extra": {
      "eas": {
        "projectId": "b734f6c0-cf67-4e15-a636-591cab14099f"
      }
    }
  }
}
